            </ComboBox>
            <Label x:Name="lblStkDots" Content="Stake Dots" HorizontalAlignment="Left" Margin="44,97,0,0" VerticalAlignment="Top" FontWeight="Bold" IsEnabled="False" SnapsToDevicePixels="True"/>

            <Label x:Name="lblHolesRDist" Content="Right" HorizontalAlignment="Left" Margin="26,350,0,0" VerticalAlignment="Top" FontWeight="Bold" SnapsToDevicePixels="True" IsEnabled="False" HorizontalContentAlignment="Right"/>
            <Button x:Name="btnTBLink" HorizontalAlignment="Left" Margin="117,288,0,0" VerticalAlignment="Top" Height="14" Width="14" FontSize="8" IsEnabled="False">
                <TextBlock x:Name="buttonTextBlock" FontFamily="Segoe MDL2 Assets" Text="&#xE72E;" />
            </Button>
            <Button x:Name="btnLRLink" HorizontalAlignment="Left" Margin="117,342,0,0" VerticalAlignment="Top" Height="14" Width="14" FontSize="8" IsEnabled="False">
                <TextBlock x:Name="buttonTextBlock1" FontFamily="Segoe MDL2 Assets" Text="&#xE72E;" />

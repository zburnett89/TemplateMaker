        xmlns:local="clr-namespace:ChatGPT_Temp_Code"
        Icon="Template Maker Icon.ico"
        mc:Ignorable="d"
        Title="Create Template" Width="1200" MinWidth="605" MinHeight="480" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen">

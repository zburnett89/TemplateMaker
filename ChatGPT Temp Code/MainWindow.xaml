            <TextBox x:Name="txtTDist" HorizontalAlignment="Left" Margin="73,273,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40" Height="20" IsEnabled="False"/>
            <Label x:Name="lblHolesTDist" Content="Top" HorizontalAlignment="Left" Margin="36,270,0,0" VerticalAlignment="Top" FontWeight="Bold" SnapsToDevicePixels="True" IsEnabled="False" HorizontalContentAlignment="Right"/>

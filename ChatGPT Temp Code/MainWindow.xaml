            <CheckBox x:Name="ckbxCTBGroms" Content="Center Top &amp; Bottom" HorizontalAlignment="Left" Margin="279,193,0,0" VerticalAlignment="Top" IsEnabled="False"/>
            <CheckBox x:Name="ckbxCLRgroms" Content="Center Left &amp; Right" HorizontalAlignment="Left" Margin="279,218,0,0" VerticalAlignment="Top" IsEnabled="False"/>
            <CheckBox x:Name="ckbxTBspacing" Content="Every" HorizontalAlignment="Left" Margin="279,243,0,0" VerticalAlignment="Top" IsEnabled="False"/>
            <TextBox x:Name="txtTBspacing" HorizontalAlignment="Left" Margin="334,243,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40" Height="18" ScrollViewer.CanContentScroll="True" HorizontalScrollBarVisibility="Auto" IsEnabled="False"/>
            <TextBlock HorizontalAlignment="Left" Margin="381,243,0,0" TextWrapping="Wrap" Text="in. Along" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="299,268,0,0" TextWrapping="Wrap" Text="Top &amp; Bottom" VerticalAlignment="Top"/>
            <CheckBox x:Name="ckbxLRspacing" Content="Every" HorizontalAlignment="Left" Margin="279,293,0,0" VerticalAlignment="Top" IsEnabled="False"/>
            <TextBox x:Name="txtLRspacing" HorizontalAlignment="Left" Margin="334,293,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="40" Height="18" IsEnabled="False"/>
            <TextBlock HorizontalAlignment="Left" Margin="381,293,0,0" TextWrapping="Wrap" Text="in. Along" VerticalAlignment="Top"/>
            <TextBlock HorizontalAlignment="Left" Margin="299,318,0,0" TextWrapping="Wrap" Text="Left &amp; Right" VerticalAlignment="Top"/>

                <ComboBoxItem Content="6x9"/>
                <ComboBoxItem Content="6x12"/>
                <ComboBoxItem Content="6x24"/>
                <ComboBoxItem Content="12x18"/>
                <ComboBoxItem Content="12x24"/>
                <ComboBoxItem Content="18x24"/>
                <ComboBoxItem Content="24x24"/>
                <ComboBoxItem Content="24x36"/>
                <ComboBoxItem Content="24x48"/>
                <ComboBoxItem Content="32x48"/>
